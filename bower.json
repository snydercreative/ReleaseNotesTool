{
  "name": "releasenotestool",
  "description": "A tool to email out formatted release notes written in Markdown.",
  "main": "index.js",
  "authors": [
    "Michael Snyder"
  ],
  "license": "MIT",
  "homepage": "https://github.com/snydercreative/ReleaseNotesTool",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "overrides": {
    "ace-builds": {
      "main": [
        "src-min-noconflict/ace.js",
        "src-min-noconflict/mode-json.js",
        "src-min-noconflict/mode-yaml.js",
        "src-min-noconflict/theme-idle_fingers.js",
        "src-min-noconflict/worker-json.js"
      ]
    }
  }
}
